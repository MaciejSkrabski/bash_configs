alias jupyter="conda run jupyter-notebook"
alias nav="conda run anaconda-navigator"
alias concode="conda run code"
alias uu="sudo apt update && sudo apt upgrade -y; sudo apt autoclean -y; sudo apt-get clean; sudo apt autoremove -y"
alias cs=cdls
alias brc="source ~/.bashrc"
alias key="sudo inxi -Fxs >> keyerror.txt; uname -srv >> keyerror.txt; xinput >> keyerror.txt"
alias cls="clear"
alias shutdown="systemctl poweroff"
alias poweroff="systemctl poweroff"
alias reboot="systemctl reboot"
