spongebob() { 
    python3.8 ~/.spongebob.py "$1" 
}

cdls() {
    if [ $# -eq 1 ]
    then
    	cd "$1" && ls .
    else
    	cd ~ && ls .
    fi
}

snapclear() {
snap list --all | awk '/disabled/{print $1, $3}' |
	    while read snapname revision; do
		sudo snap remove "$snapname" --revision="$revision"
	    done
}
